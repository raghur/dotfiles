{
    "security.workspace.trust.untrustedFiles": "open",
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        // leader mappings
        {
            "before": [ "<leader>", "b" ],
            "commands": [ "workbench.action.tasks.runTask" ]
        },
        {
            "before": [ "<leader>", "w" ],
            "commands": [ ":wall" ]
        },
        {
            "before": [ "<leader>", "h" ],
            "commands": [ ":nohl" ]
        },
        {
            "before": [ "<leader>", "<space>", ],
            "commands": [ "workbench.action.quickOpen" ]
        },
        {
            "before": [ "<leader>", ".", ],
            "commands": [ "editor.action.quickFix" ]
        },
        {
            "before": [ "<leader>", "r", ],
            "commands": [ "editor.action.rename" ]
        },
        // g mappings
        {
            "before": [ "g", "t", ],
            "commands": [ "workbench.action.showAllSymbols" ]
        },
        {
            "before": [ "g", "]", ],
            "commands": [ "editor.action.marker.nextInFiles" ]
        },
        {
            "before": [ "g", "[", ],
            "commands": [ "editor.action.marker.previousInFiles" ]
        },
        {
            "before": [ "g", "d", ],
            "commands": [ "editor.action.revealDefinition" ]
        },
        {
            "before": [ "g", ".", ],
            "commands": [ "editor.action.refactor.preview" ]
        },
        {
            "before": [ "g", "r", ],
            "commands": [ "editor.action.goToReferences" ]
        },
        {
            "before": [ "g", "o", ],
            "commands": [ "workbench.action.gotoSymbol" ]
        },
        {
            "before": [ "g", "e", ],
            "commands": [ "workbench.action.compareEditor.openSide" ],
        },
        {
            "before": [ "ctrl", "o", ],
            "commands": "workbench.action.navigateBack"
        },
        {
            "before": [ "ctrl", "i", ],
            "commands": "workbench.action.navigateForward"
        },
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "redhat.telemetry.enabled": false,
    "git.confirmSync": false,
    "typescript.tsserver.experimental.enableProjectDiagnostics": true,
    "vim.normalModeKeyBindings": [
    
    ],
    "gitlens.views.searchAndCompare.files.layout": "tree",
    "jasmineExplorer.nodeArgv": [
        " [\"-r\", \"ts-node/register\"]"
    ],
    "editor.minimap.autohide": true,
    "javascript.referencesCodeLens.showOnAllFunctions": true,
    "javascript.preferGoToSourceDefinition": true,
    "typescript.experimental.tsserver.web.enableProjectWideIntellisense": true,
    "typescript.preferGoToSourceDefinition": true,
    "typescript.inlayHints.variableTypes.suppressWhenTypeMatchesName": false,
    "yaml.schemas": {
        "file:///home/raghu/.vscode/extensions/atlassian.atlascode-3.8.0/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "workbench.iconTheme": "vscode-icons",
    "hediet.vscode-drawio.resizeImages": null,
    "dotnetAcquisitionExtension.enableTelemetry": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "karmaTestExplorer.autoWatchBatchDelay": 30,
    "python.analysis.extraPaths": [
        "/home/raghu/.vscode/extensions/continue.continue-0.6.15-linux-x64"
    ],
    "python.autoComplete.extraPaths": [
        "/home/raghu/.vscode/extensions/continue.continue-0.6.15-linux-x64"
    ],
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "eslint.useESLintClass": true,
    "eslint.lintTask.enable": true,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "karmaTestExplorer.enableExtension": false,
    "karmaTestExplorer.env": {
        "CHROME_BIN": "/usr/bin/chromium"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "githubPullRequests.pushBranch": "always",
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "karmaTestExplorer.allowGlobalPackageFallback": false,
    "vim.showMarksInGutter": true,
    "scm.alwaysShowRepositories": true,
    "C_Cpp.intelliSenseEngine": "disabled",
    "workbench.colorTheme": "Default Light+",
    "windowColors.repoConfigurationList": [
        "Rockwell-Automation-Inc/LeMans-Common-Admin: #5cb514",
        "Rockwell-Automation-Inc/LeMans-Home-UI: #043759"
    ],
    "comments.thread.confirmOnCollapse": "never",
    "workbench.sideBar.location": "right",
    // "github.copilot.advanced": {
    //     "debug.overrideEngine": "",
    //     "debug.overrideModel": "",
    //     "debug.showScores": true,
    //     "debug.testOverrideProxyUrl": "http://localhost:8000",
    //     "debug.overrideProxyUrl": "http://localhost:8000",
    // }
}
